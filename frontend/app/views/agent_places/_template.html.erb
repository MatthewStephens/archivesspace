<% define_template "agent_place", jsonmodel_definition(:agent_place) do |form| %>
  <div class="subrecord-form-fields">
    <div class="agent-record-control-container">
      <%= form.label_and_select "place_role_enum", [""] + form.possible_options_for("place_role_enum", false) %>

			<%= render_aspace_partial :partial => "shared/subrecord_form", :locals => {:form => form, :name => "subjects", :template_erb => "subjects/template", :template => "subrecord_subject_agent_place", :heading_text => "Place Subject", :heading_size => "h4"} %>

	    <%= render_aspace_partial :partial => "notes/form", :locals => {:form => form, :all_note_types => note_types_for("agent_place"), :section_id => "agent_place_notes", :nested_note_jsonmodel => "agent_place", :header_size => "h4"} %>

      <%= render_aspace_partial :partial => "shared/subrecord_form", :locals => {:form => form, :name => "dates", :template_erb => "dates/template", :template => "structured_date_label", :heading_text => I18n.t("name._frontend.section.dates"), :heading_size => "h4"} %>

    </div>
  </div>
<% end %>